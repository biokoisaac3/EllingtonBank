{
  "expo": {
    "name": "Ellington Personal Banking",
    "slug": "ellingtonbank",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/Logo2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ellingtonmfb.app",
      "buildNumber": "21",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "package": "com.ellingtonmfb.app",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/Logo2.png",
        "backgroundColor": "#3F401B"
      },
      "permissions": ["com.google.android.gms.permission.AD_ID"]
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/logo.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./app/assets/logo.png",
          "resizeMode": "cover",
          "backgroundColor": "#3F401B",
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": []
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "ndkVersion": "26.1.10909125",
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "abiFilters": ["arm64-v8a"]
          }
        }
      ],
      "./plugins/withAppsFlyerPreinstall"
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "2cb6bacc-1e05-4771-81c3-6a9934f26c7d"
      }
    }
  }
}
